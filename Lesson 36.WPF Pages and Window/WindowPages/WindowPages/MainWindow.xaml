<Window x:Class="WindowPages.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WindowPages"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <WrapPanel Margin="10" DockPanel.Dock="Top">
            <Button Margin="10" Padding="5" Name="btnToLogin" Click="btnToLogin_Click">
                Вхід
            </Button>
            <Button Margin="10" Padding="5" Name="btnToRegister" Click="btnToRegister_Click">
                Реєстрація
            </Button>
        </WrapPanel>

        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>
    </DockPanel>
    
</Window>
